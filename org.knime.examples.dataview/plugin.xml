<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin>

	<extension
		point="org.knime.workbench.repository.categories">
		<category
			description="Category containing my nodes"
			level-id="myNodes"
			name="My nodes"
			path="/" />
	</extension>

	<extension
		point="org.knime.workbench.repository.nodes">
		<node
			category-path="/myNodes"
			factory-class="org.knime.examples.dataview.DataViewNodeFactory" />
	</extension>

</plugin>
